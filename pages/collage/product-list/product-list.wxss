/* pages/collage/product-list/product-list.wxss */
.jn-main {
  width: 100%;
  overflow: hidden;
  margin: 0 auto;
  background-color: #fff;
  position: relative;
  padding-bottom: 100rpx;
  min-height: 100vh;
  box-sizing: border-box;
  font-family: PingFangSC-Regular;
}
.jn-bottom {
  text-align: center;
  height: 60rpx;
  width: 100vw;
  line-height: 60rpx;
}
.jn-bottom image {
  width: 80rpx;
  height: 40rpx;
}

/* 拼团start */
.collage-box {
  background-color: #fff;
}
.collage-box .collage-content-box {
  padding: 30rpx;
}
.collage-box .collage-content-box .collage-content {
  height: 280rpx;
  width: 100%;
}
.collage-box .collage-content-box .collage-content:last-of-type {
  margin-bottom: 0;
}
.collage-box .collage-content-box .collage-content .list-collage {
  height: 166rpx;
  width: 100%;
  position: relative;
  margin-bottom: 30rpx;
}
.collage-box .collage-content-box .collage-content .list-collage:last-of-type {
  margin-bottom: 0rpx;
}
.collage-box .collage-content-box .collage-content .list-collage .left-img {
  width: 320rpx;
  height: 244rpx;
  margin-right: 30rpx;
}
.collage-box
  .collage-content-box
  .collage-content
  .list-collage
  .left-img
  image {
  width: 100%;
  height: 100%;
}
.collage-box .collage-content-box .collage-content .list-collage .right-infor {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #000000;
  letter-spacing: 0;
}
.name-collage {
  font-size: 32rpx;
  color: #111111;
}
.price-box {
  font-size: 28rpx;
  color: #336964;
  line-height: 76rpx;
  height: 76rpx;
}
.price-box .price-precent {
  font-size: 40rpx;
  color: #D43737;
}
.price-box .price-origin {
  font-size: 28rpx;
  color: #888;
  margin-left: 12rpx;
  text-decoration: line-through;
}
.detail-num  {
  color: #666666;
  font-size:22rpx;
}
.go-btn {
  font-size: 26rpx;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2rpx;
  width: 132rpx;
  height: 55rpx;
  line-height: 55rpx;
  background: #fff;
  text-align: center;
  color: #336964;
  margin-top: 16rpx;
  border: 1rpx solid #336964;
  padding-right: 20rpx;
  position: relative;
}
.icon-more {
  font-size: 30rpx;
  position: absolute;
  top: -2rpx;
}
/* 拼团end */
