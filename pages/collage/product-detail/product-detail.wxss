page {
  font-family: PingFangSC-Regular;  
}
.icon-rightblue {
  color: #999;
  font-size: 30rpx;
}
.product-img {
  width: 100vw;
  height: 520rpx;
  display:block;
}
.product-img image {
  width: 100%;
  height: 520rpx;
}
.product-box {
    background-color: #fff;
    padding: 20rpx 30rpx;
    box-sizing: border-box;
    height: 220rpx
}
.product-name {
  font-size: 32rpx;
}
.share-btn {
  width: 50rpx;
  height: 50rpx;
  background-color: #fff;
  border: none;
  outline: none;
  float: right;
  text-align: center;
  line-height: 40rpx;
  padding: 0;
  margin: 0;
  margin-top: 70rpx;
}
.share-btn::after {
  border: none;
}
 .icon-fenxiang {
  font-size: 44rpx;
  color: #999999;
} 
.price-box{
    font-size: 36rpx;
    color: #ff6600;
    line-height: 80rpx;
    height: 90rpx;
}
.price-box .current {
    font-size: 76rpx;
}
.price-box .yuanjia {
  font-size: 28rpx;
  color: #888888;
  padding-left: 8rpx;
  text-decoration: line-through;
}
.collage-num {
  font-size: 26rpx;
  color: #888888;
}

.collage-list {
  padding: 30rpx;
  background-color: #fff;
  padding-top: 10rpx;
}
.collage-list .bd {
  padding-bottom: 16rpx;
}
.collage-list .sub-title {
  border-bottom: 2rpx solid #f3f3f3;
  font-size: 30rpx;
  height: 88rpx; 
  line-height: 88rpx;
  color: #111111;
  /* padding: 0 20rpx; */
}
.collage-list .li {
  height: 120rpx;
  overflow: hidden;
  /* padding-left: 20rpx; */
  padding-bottom: 2rpx;
}
.collage-list .li image {
  float: left;
  width: 100rpx; 
  height:100rpx;
  margin-top: 10rpx;
  margin-right: 20rpx;
  border-radius: 50%;
}
.collage-list .li .collage-product {
  float: left;
  margin-top: 20rpx;
}
.collage-product-name {
  line-height: 28rpx;
  font-size: 28rpx;
  color: #333333;
  display: inline-block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  max-width: 380rpx;
  font-weight: bold;
}
.collage-product-num {
  font-size: 26rpx;
  color: #888;
  line-height: 20rpx;
}
.collage-btn {
  float: right;
  /* margin-right: 20rpx; */
  width: 132rpx; 
  height: 56rpx; 
  line-height: 56rpx;
  text-align: center;
  background: #ff6600;
  color: #FFFFFF;
  border-radius: 200rpx; 
  margin-top: 30rpx;
  font-size: 26rpx;
}

.collage-successed-tip {
  text-align: center;
  color: #888;
  padding-top: 30rpx;
  font-size: 24rpx;
}

.store-box {
  background-color: #fff;
}
.store-box .sub-title {
  padding: 0 30rpx;
  border-bottom: 2rpx solid #f3f3f3;
  height: 88rpx;
  line-height: 88rpx;
  color: #111111;
}
.store-name {
  height: 90rpx;
  line-height: 90rpx;
  font-size: 30rpx;
  color: #000;
  padding: 0 30rpx;  
}
