.container2 {
    background-color: #fff;
}
.container2 .icon-weizhix {
  display: inline;
}

.address-list {
    padding: 0 30rpx;
}
.address-box {
  font-size: 30rpx;
  color: #333333;
    padding: 12rpx 0;
    border-bottom: 1px solid #f3f3f3;
    line-height: 40rpx;
}
.address-box-active {
    background-color: #f5f5f5;
}
.address-detail {
   font-size: 26rpx;
   color: #999999;
}
.buttons{
  position:fixed;
  bottom:0;
  width:100%;
  height:100rpx;
  line-height:100rpx;
  background:#336964;
  color:#fff;
  text-align:center;
}