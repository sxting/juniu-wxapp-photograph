.icon-tel {
  font-size: 36rpx;
  color: #999999;  
}
.tel-box {
  height: 36rpx;
  border-left: 2rpx solid #e1e1e1;
  padding-left: 30rpx;
  line-height: 36rpx;
  text-align: right;
  margin-top: 26rpx;
  width: 60rpx;
  box-sizing: border-box;
}

.product-detail, .activity-notice {
  padding: 0 30rpx 20rpx;
  box-sizing: border-box;  
  width: 700rpx;
  margin: 30rpx auto 0;
}
.product-detail-text, .activity-notice-text {
  /* height: 80rpx; */
  line-height: 80rpx;
}
.activity-notice-text-title1 {
  font-size: 30rpx;
}
.activity-notice-text-title {
  line-height: 50rpx;
}
.notice {
  color: #111111;
  /* font-size: 30rpx; */
}
.product-detail image {
  width: 650rpx;
  height: 300rpx;
  display: block;
  margin-bottom: 20rpx;
  border-radius: 8rpx;
}

.activity-notice {
  margin: 30rpx auto;
}
.notice-item {
  font-size: 26rpx;
  color: #999999;
}

.margin-b130 {
  /* margin-bottom: 130rpx; */
  padding-bottom: 130rpx;
}

.disabled {
  background: #C5C5C5;
  color: #FFFFFF;
}

.shared-one text:first-child, .shared-two text:first-child, .shared-three text:first-child {
  width: 40%;
  background: #353A41;
  display: inline-block;
}
.shared-one text:last-child, .shared-two text:last-child, .shared-three text:last-child {
  display: inline-block;
}

.comment-box {
  margin-top: 0;
}
.comeBackHome {
   bottom:65%;
 }

