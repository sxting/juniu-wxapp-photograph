.top-bg {
    width: 100vw;
    height: 144rpx;
}
.top-bg>image {
  width: 100vw;
  height: 144rpx;
  position: absolute;
  top: 0;
  left: 0;
}
.personal {
  width: 700rpx;
  height: 188rpx;
  position: absolute;
  top: 26rpx;
  left: 25rpx;
  z-index: 100;
  background: #FFFFFF;
  box-shadow: 0 1px 3px 0 #D8D8D8;
  border-radius: 8px;
}
.personal-image {
    width: 130rpx;
    height: 130rpx;
    margin-left: 30rpx;
    border-radius: 50%;
    margin-top: 26rpx; 
}
.personal-message {
    display: inline-block;
    color: #111111;    
    margin-top: 40rpx;
    margin-left: 20rpx; 
}
.nickname {
    font-size: 34rpx;
    padding-bottom: 10rpx;
}
.tel {
  font-size: 28rpx;
}

.my-order {
  height: 260rpx;
  width: 700rpx;
  margin: 100rpx auto 30rpx;
}
.my-order image {
  width: 56rpx;
  height: 56rpx;
}
.subtitle {
  height: 88rpx;
  line-height: 88rpx;
  padding-left: 30rpx;
  font-size: 30rpx;
  color: #111111;
  border-bottom: 2rpx solid #F3F3F3;
}
.order-list {
  display: flex;
}
.order-item {
  width: 20%;
  font-size: 26rpx;
  color: #666666;
  text-align: center;
  padding-top: 34rpx;
}

.list-box {
  width: 690rpx;
  margin: 0 auto;
}
.my-comment, .my-appoint {
    height: 96rpx;
    line-height: 96rpx;
    background-color: #fff;
    padding-right: 30rpx;
    padding-left: 130rpx;
    border-bottom: 2rpx solid #F3F3F3;
    box-sizing: border-box;
    font-size: 26rpx;
    color: #111111;
    position: relative;
}
.my-comment image, .my-appoint image {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  top: 16rpx;
  left: 30rpx;
}
.icon-opentime, .icon-Evaluation {
    display: inline-block;
    font-size: 34rpx;
    color: #D9D9D9;
    padding-right: 10rpx;
}
.icon-Evaluation {
    color: #e5e5e5;
}
.icon-rightblue {
    font-size: 26rpx;
    color: #c7c7c7;
    text-align: right;
    float: right;
}
.go-back-btn {
    width: 690rpx;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    border: 1px solid #ff6600;
    border-radius: 4px;
    color: #ff6600;
    font-size: 32rpx;
    margin: 100rpx auto;
}

.version {
  text-align:center;
  color:#e5e5e5;
  background-color:#fff;
  font-size:24rpx;
  opacity:0.8;
  position:absolute;
  width:100vw;
  bottom:90rpx;
}
