@import 'style/weui.wxss';
@import 'style/common.wxss';

.jn-color {
  color: #336964;
}

.w-inner {
  width: 690rpx;
  margin: 0 auto;
}

.jn-bgc {
  background-color: #336964;
}

.mt-10 {
  margin-top: 20rpx;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.clearfix:after {
  content: '\20';
  display: block;
  height: 0;
  clear: both;
}

.clearfix {
  zoom: 1;
}

page {
  background-color: #fff;
  font-size: 34rpx;
  color: #222;
}

.container {
  font-size: 30rpx;
  font-family: "PingFangSC-Regular";
  background-color: #f5f5f5;
}

.jn-bg {
  height: 20rpx;
  background: #f8f8f8;
  width: 100vw;
}

.mb-100 {
  margin-bottom: 100rpx;
}

.jn-btn {
  background: #336964;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100vw;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2rpx;
}

.placeholder {
  font-family: "PingFangSC-Regular";
  font-size: 28rpx;
  color: #999;
}

/*优惠券样式 start  */

.ticket-block {
  padding-left: 30rpx;
  padding-bottom: 20rpx;
}

.tickte-item {
  position: relative;
  width: 690rpx;
  height: 184rpx;
  background: #fff;
  border: 1rpx solid #336964;
  margin-bottom: 20rpx;
  box-sizing: border-box;
  font-size: 22rpx;
  color: #0f2b27;
}

.tickte-item2 {
  color: #999;
  border: 1rpx solid #999;
}

.tickte-item2 .ticket-price-box {
  border-right: 1rpx dashed #999;
}

