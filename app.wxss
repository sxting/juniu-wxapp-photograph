@import 'style/weui.wxss';
@import 'style/common.wxss';

.jn-color {
  color: #336964;
}

.jn-bgc {
  background-color: #336964;
}

.mt-10 {
  margin-top: 20rpx;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.clearfix:after {
  content: '\20';
  display: block;
  height: 0;
  clear: both;
}
.clearfix {
  zoom: 1;
}

.placeholder {
  font-family: "PingFangSC-Regular";
  font-size: 28rpx;
  color: #999999;
}

/*优惠券样式 start  */
.ticket-block {
  padding-left: 30rpx;
  padding-bottom: 20rpx;
}

.tickte-item {
  position: relative;
  width: 690rpx;
  height: 184rpx;
  background: #fff;
  border: 1rpx solid #336964;
  margin-bottom: 20rpx;
  box-sizing: border-box;
  font-size: 22rpx;
  color: #0f2b27;
}

